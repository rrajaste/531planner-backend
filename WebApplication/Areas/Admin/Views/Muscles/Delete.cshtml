@using Resources.Views.Muscle
@model BLL.App.DTO.Muscle

@{
    ViewData["Title"] = "Delete";
}

<h1>@Delete.Title</h1>

<h3>@Delete.ConfirmationText</h3>
<div>

    <partial name="_DetailsDeletePartial"/>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id"/>
        <input type="submit" value="Delete" class="btn btn-danger"/> |
        <a asp-action="Index">@Delete.BackToList</a>
    </form>
</div>