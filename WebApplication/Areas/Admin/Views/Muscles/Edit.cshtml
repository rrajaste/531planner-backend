@using Resources.Views.Muscle
@model WebApplication.Areas.Admin.ViewModels.MuscleCreateEditViewModel

@{
    ViewData["Title"] = "Edit";
}

<h1>@Edit.Title</h1>

<h4>@Details.Muscle</h4>
<hr/>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <input type="hidden" asp-for="Muscle.Id"/>
            <partial name="_CreateEditPartial"/>

        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Edit.BackToList</a>
</div>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}