@using Resources.Views.Muscle
@model WebApplication.Areas.Admin.ViewModels.MuscleCreateEditViewModel

@{
    ViewData["Title"] = "Create";
}

<h1>@Create.Title</h1>

<h4>@Create.Muscle</h4>
<hr/>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <partial name="_CreateEditPartial"/>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Create.BackToList</a>
</div>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}