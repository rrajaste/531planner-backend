@model BLL.App.DTO.TrainingCycle

<input type="hidden" asp-for="WorkoutRoutineId" />
<div asp-validation-summary="All" class="text-danger"></div>
<div class="form-group">
    <label asp-for="WorkoutRoutineId" class="control-label"></label>
    <input asp-for="WorkoutRoutineId" class="form-control" />
    <span asp-validation-for="WorkoutRoutineId" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="CycleNumber" class="control-label"></label>
    <input asp-for="CycleNumber" class="form-control" />
    <span asp-validation-for="CycleNumber" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="StartingDate" class="control-label"></label>
    <input asp-for="StartingDate" class="form-control" />
    <span asp-validation-for="StartingDate" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="EndingDate" class="control-label"></label>
    <input asp-for="EndingDate" class="form-control" />
    <span asp-validation-for="EndingDate" class="text-danger"></span>
</div>