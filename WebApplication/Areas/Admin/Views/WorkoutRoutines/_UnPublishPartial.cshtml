@model WebApplication.Areas.Admin.ViewModels.WorkoutRoutinePublishViewModel

<form asp-action="UnPublish">
    <input type="hidden" asp-for="@Model.WorkoutRoutineId"/>
    <button type="submit" class="btn btn-outline-danger" 
            title="Remove routine from published routines list, users can no longer see or use this">UNPUBLISH</button>
</form>
